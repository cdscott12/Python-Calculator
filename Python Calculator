def add(num1, num2):
    """Returns num 1 plus num2."""
    return num1 + num2

def subtract(num1, num2):
    """Returns num1 minus num 2."""
    return num1 - num2

def multiply(num1, num2):
    """Returns num1 times num2."""
    return num1 * num2

def divide(num1, num2):
    """Returns num1 divided by num2"""
    return num1 / num2

def main():
    print(add(1,1)) #2
    print(subtract(3,2)) #1
    print(multiply(7, 5)) #35
    print(divide(90 ,2)) #45

main()
